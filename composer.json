{
    "name": "yii-tools/data-provider",
    "type": "library",
    "description": "Proveedores de datos para Yii3.",
    "keywords": [
        "yii",
        "Proveedores de datos"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "yii-tools/interface": "^1.0@dev",
        "yiisoft/active-record": "^3.0@dev",
        "yiisoft/db": "^1.0"
    },
    "require-dev": {
        "maglnet/composer-require-checker": "^4.3",
        "phpunit/phpunit": "^10.0",
        "roave/infection-static-analysis-plugin": "^1.29",
        "vimeo/psalm": "^5.8",
        "yiisoft/cache": "^3.0",
        "yiisoft/db-sqlite": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yii\\DataProvider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\DataProvider\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "yiisoft/config": true
        }
    },
    "scripts": {
        "check-dependencies": "composer-require-checker",
        "mutation": "roave-infection-static-analysis-plugin",
        "psalm": "psalm",
        "test": "phpunit"
    }
}
